{
  "expo": {
    "name": "Cast Finan√ßas",
    "slug": "financas-app",
    "version": "2.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/fincast.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/fincast.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544/6300978111"
      },
      "versionCode": 15,
      "package": "com.jairjr.financasapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/fincast.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/fincast.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ad7e3cb0-2d66-4b42-9463-9008b0dc7ca1"
      }
    },
    "owner": "jairjr"
  }
}
